    "@auto-it/conventional-commits": "^10.32.2",
    "@auto-it/first-time-contributor": "^10.32.2",
    "@auto-it/released": "^10.32.2",
    "@auto-it/slack": "^10.32.2",
    "auto": "^10.32.2",
    "vite": "^2.5.0",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "engines": {
    "node": "14.x"
  },
  "repository": {
